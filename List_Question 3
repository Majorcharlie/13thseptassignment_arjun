package List;
import java.util.ArrayList;

public class Question3 {

    public static void main(String[] args) {
        // Create an ArrayList of Strings
        ArrayList<String> arrayList = new ArrayList<>();
        arrayList.add("a");
        arrayList.add("r");
        arrayList.add("j");
        arrayList.add("u");
        arrayList.add("n");
        arrayList.add("s");
        arrayList.add("i");
        arrayList.add("n");
        arrayList.add("g");
        arrayList.add("h");

        
        arrayList.add("p");
        arrayList.add("h");
        arrayList.add("u");
        arrayList.add("n");
        arrayList.add("c");

        
        arrayList.subList(0, 5).clear();

        // Print the list
        System.out.println(arrayList);
    }
}

